<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pap.controllers.BrowseBookHistoryController">
   <children>
      <TableView fx:id="rentalCatalog" tableMenuButtonVisible="true">
        <columns>
          <TableColumn fx:id="dateReturned" maxWidth="-1.0" prefWidth="-1.0" text="Date of returning" />
          <TableColumn fx:id="dateToReturn" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Expected return date" />
            <TableColumn fx:id="dateRented" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Rental date" />
            <TableColumn fx:id="userId" maxWidth="-1.0" prefWidth="-1.0" text="UserID" />
            <TableColumn fx:id="userFirstName" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="User's first name" />
            <TableColumn fx:id="userLastName" maxWidth="-1.0" prefWidth="-1.0" text="User's last name" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#goBack" text="Go back" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
