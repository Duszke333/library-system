<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<SplitPane dividerPositions="0.5" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pap.controllers.UserDashboardController">
   <items>
      <ListView fx:id="userActions" />
      <VBox fx:id="contentPane" alignment="TOP_CENTER">
         <children>
            <ScrollPane>
               <content>
                  <Text fx:id="loginInfo" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="100" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </items>
</SplitPane>
