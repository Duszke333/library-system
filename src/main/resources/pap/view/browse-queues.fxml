<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="queueCatalog" onMouseClicked="#getItem" tableMenuButtonVisible="true" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pap.controllers.BrowseQueuesController">
  <columns>
      <TableColumn fx:id="ready" maxWidth="-1.0" prefWidth="-1.0" text="Ready to pickup" />
    <TableColumn fx:id="pickupDate" maxWidth="-1.0" prefWidth="-1.0" text="Expected pickup date" />
    <TableColumn fx:id="bookTitle" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Title" />
    <TableColumn fx:id="bookAuthor" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Author" />
    <TableColumn fx:id="bookLanguage" maxWidth="-1.0" prefWidth="-1.0" text="Language" />
  </columns>
</TableView>
